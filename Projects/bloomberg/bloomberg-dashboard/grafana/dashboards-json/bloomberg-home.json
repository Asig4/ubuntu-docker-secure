      "title": "TOKYO",
      "gridPos": { "h": 2, "w": 5, "x": 15, "y": 23 },
      "transparent": true,
      "options": {
        "mode": "time",
        "clockType": "24 hour",
        "timezone": "Asia/Tokyo",
